{
    "autoload": {
        "psr-4": {
            "GoF\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "require": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "cs-check": [
            "@php ./vendor/bin/php-cs-fixer fix --dry-run --diff --ansi"
        ],
        "cs-fix": [
            "@php ./vendor/bin/php-cs-fixer fix"
        ]
    }
}
